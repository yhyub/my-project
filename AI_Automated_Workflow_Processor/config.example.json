{
    "deepseek_api_key": "your_deepseek_api_key_here",
    "deepseek_api_url": "https://api.deepseek.com",
    "model": "deepseek-chat",
    "timeout": 60,
    "output_dir": "./workflow_output",
    "log_level": "info",
    "max_batch_size": 100,
    "auto_save": true,
    "enable_code_analysis": true,
    "enable_workflow_generation": true,
    "enable_auto_fix": true,
    "code_analysis_config": {
        "max_line_length": 80,
        "enable_long_line_check": true,
        "enable_duplicate_code_check": true,
        "enable_docstring_check": true
    },
    "workflow_generation_config": {
        "default_model": "deepseek-ai/DeepSeek-R1-0528-Qwen3-8B",
        "temperature": 0.7,
        "max_tokens": 2048
    },
    "auto_fill_canvas_config": {
        "default_theme": "default",
        "auto_layout": true,
        "node_spacing": 200,
        "edge_style": "curved"
    }
}